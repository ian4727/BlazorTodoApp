<main class="main">
    <ul class="todo-list">
        @foreach (var pair in FilterItems(Items))
        {
            var id = pair.Key;
            var todoItem = pair.Value;
            
            if(ItemTemplate != null)
            {
                @ItemTemplate(GetTodoItemTemplateData(todoItem))
            }
            else if (ItemTemplate2 != null)
            {
                @ItemTemplate2(new TodoItemTemplateData2(todoItem))
            }
            else
            {
                <li>Please provide a Template</li>
            }
        }
    </ul>
</main>




